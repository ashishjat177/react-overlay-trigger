{
  "name": "reactip",
  "version": "0.0.1",
  "description": "react tooltip component",
  "main": "lib/index.js",
  "scripts": {
    "build:demo": "npm run clean:demo && NODE_ENV=production webpack --colors --progress --config demo/webpack.config.js",
    "ghpages": "npm run build:demo && gh-pages -d demo/dist",
    "clean:demo": "rm -rf demo/dist",
    "build": "NODE_ENV=production webpack --colors --progress --config webpack.config.js",
    "start": "webpack-dev-server --progress --colors --content-base demo --config demo/webpack.config.js --port 3000",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "*.md",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huozhi/reactip.git"
  },
  "keywords": [
    "react",
    "tooltip"
  ],
  "author": "huozhi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huozhi/reactip/issues"
  },
  "homepage": "https://github.com/huozhi/reactip#readme",
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.24.1",
    "postcss-loader": "^1.2.1",
    "postcss-nested": "^1.0.0",
    "react": "*",
    "react-dom": "*",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
